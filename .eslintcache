filePath":"193","messages":"194","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"195","usedDeprecatedRules":"84"},"/Users/monicakay/workspace/neighborly/src/index.js",[],["196","197"],"/Users/monicakay/workspace/neighborly/src/components/Neighborly.js",["198","199"],"import React, {useContext, useState} from \"react\"\nimport { Route, Redirect } from \"react-router-dom\"\nimport { ApplicationViews } from \"./ApplicationViews\"\nimport { Login } from \"./auth/Login\"\nimport { Register } from \"./auth/Register\"\nimport \"./Neighborly.css\"\nimport { UserProvider } from \"./users/UserProvider\"\n\nexport const Neighborly = () => {\n    return(<>\n        <Route render={() => {\n            if (localStorage.getItem(\"token\")) {\n\n                return (\n                    <>\n                    <UserProvider>\n                        <Route render={props =>\n                            <ApplicationViews\n                            {...props}  />} />\n\n                    </UserProvider>\n                    </>\n                )\n            }\n            else {\n                return <Redirect to=\"/login\" />\n            }\n        }} />\n\n        <Route path=\"/login\" render={(props) => {\n            if (localStorage.getItem(\"token\")) {\n                return <Redirect to=\"/rare\" />\n            } else {\n                return (\n                <>\n                <UserProvider>\n                    <Login {...props} />\n                </UserProvider>\n                </>\n                )\n            }\n        }} />\n\n        <UserProvider>\n            <Route path=\"/register\" render={(props) => {\n                if (localStorage.getItem(\"token\")) {\n                    return <Redirect to=\"/rare\" />\n                }\n                else {\n                    return <Register {...props}/>\n                }\n            }} />\n        </UserProvider>\n    </>)\n}\n\n\n","/Users/monicakay